{
  "name": "@juit/lib-statvfs",
  "version": "1.0.0",
  "type": "commonjs",
  "description": "Simple native bindings for \"statvfs\"",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "scripts": {
    "build-native": "./native/build.sh",
    "build": "plug"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "df",
    "disk",
    "monitoring",
    "statvfs",
    "typescript"
  ],
  "author": "Juit Developers <developers@juit.com>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@plugjs/build": "^0.1.0",
    "@types/node": "^18.13.0",
    "node-gyp": "^9.3.1",
    "typescript": "^4.9.5"
  },
  "directories": {
    "test": "test"
  },
  "files": [
    "*.md",
    "dist/",
    "src/",
    "tsconfig.json"
  ]
}
